<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="622.0" prefWidth="952.0" xmlns="http://javafx.com/javafx/19"
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="projectmanagementlisof.controller.FXMLDeveloperLandingController">
   <children>
      <BorderPane prefHeight="622.0" prefWidth="952.0" AnchorPane.bottomAnchor="0.0"
         AnchorPane.topAnchor="0.0">
         <left>
            <AnchorPane prefHeight="654.0" prefWidth="204.0" style="-fx-background-color: #000000;"
               BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="27.0" layoutY="40.0"
                     pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/UserIcon.png" />
                     </image>
                  </ImageView>
                  <Button layoutX="14.0" layoutY="219.0" mnemonicParsing="false"
                     onAction="#btnShowActivies" prefHeight="46.0" prefWidth="179.0"
                     stylesheets="@../css/Style.css" text="Actividades">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Button>
                  <Button layoutX="14.0" layoutY="279.0" mnemonicParsing="false"
                     onAction="#btnShowChanges" prefHeight="46.0" prefWidth="179.0"
                     stylesheets="@../css/Style.css" text="Cambios">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Button>
                  <Button layoutX="14.0" layoutY="339.0" mnemonicParsing="false"
                     onAction="#btnShowChangeRequests" prefHeight="46.0" prefWidth="179.0"
                     stylesheets="@../css/Style.css" text="Solicitud de cambio">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
                  <Button layoutX="14.0" layoutY="399.0" mnemonicParsing="false"
                     onAction="#btnShowDefects" prefHeight="46.0" prefWidth="179.0"
                     stylesheets="@../css/Style.css" text="Defectos">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Button>
                  <ImageView fitHeight="49.0" fitWidth="47.0" layoutX="75.0" layoutY="552.0"
                     onMouseClicked="#btnLogOut" pickOnBounds="true" preserveRatio="true"
                     style="-fx-cursor: hand;">
                     <image>
                        <Image url="@../images/LogOutIcon.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </left>
      </BorderPane>
   </children>
</AnchorPane>